{
  "name": "marix",
  "version": "1.0.16",
  "description": "Marix - Modern Zero-Knowledge SSH Client with SFTP, FTP, RDP, WSS support",
  "main": "dist/main/index.js",
  "homepage": "https://github.com/marixdev/marix",
  "repository": {
    "type": "git",
    "url": "https://github.com/marixdev/marix.git"
  },
  "bugs": {
    "url": "https://github.com/marixdev/marix/issues"
  },
  "overrides": {
    "tar": "^6.2.1"
  },
  "scripts": {
    "postinstall": "electron-rebuild -f -w sqlite3 || true",
    "inject-build-info": "node scripts/inject-build-info.js",
    "copy-build-info": "cp src/build-info.json dist/main/build-info.json 2>/dev/null || copy src\\build-info.json dist\\main\\build-info.json 2>nul || true",
    "rebuild-native": "electron-rebuild -f -w sqlite3",
    "build:main": "tsc --project tsconfig.main.json",
    "build:renderer": "node --max-old-space-size=4096 ./node_modules/webpack/bin/webpack.js --mode production",
    "build": "npm run inject-build-info && npm run build:main && npm run copy-build-info && npm run build:renderer",
    "start": "npm run build && electron . --disable-renderer-backgrounding --js-flags=\"--max-old-space-size=256\"",
    "start:quick": "electron . --disable-renderer-backgrounding --js-flags=\"--max-old-space-size=256\"",
    "dev": "webpack serve --mode development",
    "dev:electron": "electron . --enable-logging",
    "package": "npm run build && electron-builder",
    "package:linux": "npm run build && electron-builder --linux",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac",
    "package:all": "npm run build && electron-builder --linux --win --mac",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config jest.config.js",
    "test:components": "jest --config jest.config.js --testPathPattern=components",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --watch",
    "lint": "eslint src --ext .ts,.tsx || true"
  },
  "keywords": [
    "ssh",
    "sftp",
    "ftp",
    "rdp",
    "terminal",
    "electron",
    "remote-desktop",
    "ssh-client",
    "file-manager"
  ],
  "author": "datvuong166",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/ssh2": "^1.15.5",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "html-webpack-plugin": "^5.6.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss-loader": "^8.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-cpp": "^6.0.3",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-go": "^6.0.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-php": "^6.0.2",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/lang-rust": "^6.0.2",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.12.1",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/search": "^6.5.11",
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.11",
    "@types/react-window": "^1.8.8",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "basic-ftp": "^5.1.0",
    "bonjour-service": "^1.3.0",
    "cloudflare": "^5.2.0",
    "electron-store": "^11.0.2",
    "googleapis": "^170.1.0",
    "hash-wasm": "^4.12.0",
    "mongodb": "^7.0.0",
    "mysql2": "^3.16.1",
    "node-pty": "^1.1.0",
    "node-rdpjs-2": "^0.3.5",
    "pg": "^8.17.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-window": "^2.2.5",
    "redis": "^5.10.0",
    "sqlite3": "^5.1.7",
    "ssh2": "^1.17.0",
    "ttty": "^1.7.0",
    "ws": "^8.19.0"
  }
}
